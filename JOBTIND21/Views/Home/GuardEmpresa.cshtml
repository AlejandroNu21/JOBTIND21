@model JOBTIND21.Dominio.Empresa
@{
    ViewData["Title"] = "GuardEmpresa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GuardEmpresa</h1>

@using (Html.BeginForm("GuardEmpresa", "Home", FormMethod.Post))
{


    <h2 class=" animate__animated animate__bounce">Bienvenido, Ingrese los datos de la empresa para registrarla</h2>
    <br>
    <i class="fas fa-book"></i>
    <label>Nombre de empresa</label>
    <br />
    @Html.TextBoxFor(m => m.Nombre_Empresa)
    @Html.ValidationMessageFor(m => m.Nombre_Empresa)
    <br />
    <br />
    <i class="far fa-star"></i>
    <br />
    <label>Telefono</label>
    <br />
    @Html.TextBoxFor(m => m.TelefonoEmp)
    @Html.ValidationMessageFor(m => m.TelefonoEmp)
    <br />
    <br />
    <i class="fas fa-book"></i>
    <label>Email</label>
    <br />
    @Html.TextBoxFor(m => m.EmailEmp)
    @Html.ValidationMessageFor(m => m.EmailEmp)
    <br />
    <br />

    <i class="fas fa-book"></i>
    <label>Contraseña</label>
    <br />
    @Html.TextBoxFor(m => m.ContraseñaEmp)
    @Html.ValidationMessageFor(m => m.ContraseñaEmp)
    <br />
    <br />

    <i class="fas fa-book"></i>
    <label>Vacante</label>
    <br />
    @Html.TextBoxFor(m => m.Vacante)
    @Html.ValidationMessageFor(m => m.Vacante)
    <br />
    <br />

    <i class="fas fa-book"></i>
    <label>Categoria</label>
    <br />
    @Html.TextBoxFor(m => m.CategoriaID)
    @Html.ValidationMessageFor(m => m.CategoriaID)
    <br />
    <br />
    <input type="submit">
    <br>
}
<h2 class="wow animate__animated animate__pulse" data-wow-duration="2s" data-wow-delay="2s">Guardar</h2>
<script src="https://cdn.boomcdn.com/libs/wow-js/1.3.0/wow.js"></script>
<script type="text/javascript">
    new WOW().init();
</script>

