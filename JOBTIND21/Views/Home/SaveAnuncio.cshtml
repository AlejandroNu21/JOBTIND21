@model JOBTIND21.Dominio.Anuncio
@{
    ViewData["Title"] = "SaveAnuncio";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>SaveAnuncio</h1>


@using (Html.BeginForm("SaveAnuncio", "Home", FormMethod.Post))
{


    <h2 class=" animate__animated animate__bounce">Ingrese su Anuncio por favor.</h2>
    <br>
    <i class="fas fa-book"></i>
    <label>Anuncio</label>
    <br />
    @Html.TextBoxFor(m => m.Anuncios)
    @Html.ValidationMessageFor(m => m.Anuncios)
    <br />
    <br />
    <i class="far fa-star"></i>
    <br />
    <label>Usuario</label>
    <br />
    @Html.TextBoxFor(m => m.UsuarioID)
    @Html.ValidationMessageFor(m => m.UsuarioID)
    <br />
    <br />
    <i class="fas fa-book"></i>
    <label>Codigo id de Empresa</label>
    <br />
    @Html.TextBoxFor(m => m.EmpresaID)
    @Html.ValidationMessageFor(m => m.EmpresaID)
    <br />
    <br />
    <br />
    <input type="submit">
    <br>
}
<h2 class="wow animate__animated animate__pulse" data-wow-duration="2s" data-wow-delay="2s">Guardar</h2>
<script src="https://cdn.boomcdn.com/libs/wow-js/1.3.0/wow.js"></script>
<script type="text/javascript">
    new WOW().init();
</script>
